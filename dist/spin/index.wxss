.f-spin{color:#2d8cf0;vertical-align:middle;text-align:center}.f-spin-dot{position:relative;display:block;border-radius:50%;background-color:#2d8cf0;width:20px;height:20px;animation:anf-spin-bounce 1s 0s ease-in-out infinite}.f-spin-large .f-spin-dot{width:32px;height:32px}.f-spin-small .f-spin-dot{width:12px;height:12px}.f-spin-fix{position:absolute;top:0;left:0;z-index:8;width:100%;height:100%;background-color:rgba(255,255,255,.9)}.f-spin-fullscreen{z-index:2010}.f-spin-fullscreen-wrapper{position:fixed;top:0;right:0;bottom:0;left:0}.f-spin-fix .f-spin-main{position:absolute;top:50%;left:50%;-ms-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.f-spin-fix .f-spin-dot{display:inline-block}.f-spin-show-text .f-spin-dot,.f-spin-text{display:none}.f-spin-show-text .f-spin-text{display:block;font-size:14px}@keyframes anf-spin-bounce{0%{transform:scale(0)}100%{transform:scale(1);opacity:0}}