
<f-cell-group>
    <f-cell title="基本用法">
        <f-rate 
            bind:change="onChange1" 
            value="{{starIndex1}}">
        </f-rate>
    </f-cell>
    <f-cell title="自定义星星个数">
        <f-rate
            bind:change="onChange5"
            count="{{10}}"
            value="{{starIndex5}}">
        </f-rate>
    </f-cell>
    <f-cell title="自定义星星大小">
        <f-rate 
            bind:change="onChange2" 
            value="{{starIndex2}}" 
            size="32">
        </f-rate>
    </f-cell>
    <f-cell title="自定义文字说明">
        <f-rate 
            bind:change="onChange2" 
            value="{{starIndex2}}">
            {{starIndex2}}星
        </f-rate>
    </f-cell>
     <f-cell title="手势touch选择星">
        <f-rate 
            bind:change="onChange3" 
            value="{{starIndex3}}">
            {{starIndex3}}星
        </f-rate>
    </f-cell>
    <f-cell title="禁用点击和手势选择星">
        <f-rate 
            disabled="{{true}}"
            value="{{starIndex4}}">
            {{starIndex4}}星
        </f-rate>
    </f-cell>
</f-cell-group>